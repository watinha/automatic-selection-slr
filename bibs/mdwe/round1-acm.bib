	annote = {{{\textless}p{\textgreater}Não} selecionado por não mencionar o desenvolvimento orientado a modelos no resumo, embora trate do tema de desenvolvimento web.{\textless}/p{\textgreater}},
	inserir = {false}
},

@inproceedings{kateros_methodology_2008,
